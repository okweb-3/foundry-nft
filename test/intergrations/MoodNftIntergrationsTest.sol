pragma solidity ^0.8.18;
import {Test,console} from "../../lib/forge-std/src/Test.sol";
import {MoodNft} from "../../src/MoodNft.sol";
import {DeployMoodNft} from "../../script/DepolyMoodNft.s.sol";

contract MoodNftIntergrationsTest is Test{
    MoodNft moodNft;
    string public constant HAPPY_SVG_IMG_URI="data:image/svg+xml;base64,";
    string public constant SAD_SVG_IMG_URI="data:image/svg+xml;base64,";
    string public constant SAD_SVG_URI = "data:application/json;base64,";
    address USER = makeAddr("USER");
    DeployMoodNft deployer;

    function setUp() public {
        deployer = new DeployMoodNft();
        moodNft = deployer.run();
    }
    function testViewTokenURIIntergration()public{
        vm.prank(USER);
        moodNft.mintNft();
        console.log("---",moodNft.tokenURI(0));
    }
       function testFilpMoodIntergration() public{
        vm.prank(USER);
        moodNft.mintNft();
        vm.prank(USER);
        moodNft.flipMood(0);
        assertEq(keccak256(abi.encodePacked(moodNft.tokenURI(0))),keccak256(abi.encodePacked(SAD_SVG_URI)));
    }
}